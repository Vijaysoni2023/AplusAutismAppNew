   <script type="text/javascript">
    $(document).ready(function () {
        debugger;
        if ('@ViewBag.LastestSubscriptionlist'!="")
        {
        $('#clientdetails').dataTable({
           "pagingType": "full_numbers",
           "lengthMenu": [[10, 25, 50, -1], [10, 25, 50, "All"]],
           "aoColumns": [
			{ "mDataProp": "engine" },
			{ "mDataProp": "browser" },
			{ "mDataProp": "platform" },
			{ "mDataProp": "version" },
			{ "mDataProp": "grade" }
		]
         });

        }
    });

</script>


